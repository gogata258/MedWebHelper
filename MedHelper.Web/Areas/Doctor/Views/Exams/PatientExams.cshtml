@model IEnumerable<DoctorExamConciseViewModel>
@{
	ViewData["Title"] = "Exams";
}
<h2>@ViewData["Title"]</h2>
<table class="table table-striped table-bordered">
	<thead>
		<tr>
			<td>Date</td>
			<td>Facility</td>
			<td>Status</td>
			<td>Actions</td>
		</tr>
	</thead>
	<tbody>
		@foreach (DoctorExamConciseViewModel visit in Model)
		{
			@Html.DisplayFor(Model => visit, DisplayTemplates.DOCTOR_EXAM_CONCISE)
		}
	</tbody>
</table>