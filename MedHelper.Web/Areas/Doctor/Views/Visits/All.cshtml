@model IEnumerable<DoctorVisitConciseViewModel>
@{
	ViewData["Title"] = "Appointments";
}
<h2>@ViewData["Title"]</h2>
<table class="table table-striped table-bordered">
	<thead>
		<tr>
			<td>Time</td>
			<td>Name</td>
			<td>Actions</td>
		</tr>
	</thead>
	<tbody>
		@foreach (DoctorVisitConciseViewModel visit in Model)
		{
			@Html.DisplayFor(Model => visit, DisplayTemplates.DOCTOR_VISIT_CONCISE)
		}
	</tbody>
</table>