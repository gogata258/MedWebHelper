@model IEnumerable<FacilityPersonnelViewModel>
@{
	ViewData["Title"] = "Personnel";
}

<table class="table table-striped table-bordered">
	<thead>
		<tr>
			<td>Full Name</td>
			<td>Positioned Sinde</td>
			<td>Actions</td>
		</tr>
	</thead>
	<tbody>
		@foreach (var member in Model)
		{
			@Html.DisplayFor(Model => member, DisplayTemplates.ADMIN_FACILITY_PERSONNEL)
		}
	</tbody>
</table>