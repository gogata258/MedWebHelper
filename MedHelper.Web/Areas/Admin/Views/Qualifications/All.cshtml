@model IEnumerable<QualificationConciseViewModel>
@{
	ViewData["Title"] = "All Users";
}
<h2>@ViewData["Title"]</h2>
<table class="table table-striped table-bordered ">
	<thead>
		<tr>
			<td>Name</td>
			<td>Count</td>
			<td>Actions</td>
		</tr>
	</thead>
	<tbody>
		@foreach (QualificationConciseViewModel facility in Model)
		{
			@Html.DisplayFor(model => facility, DisplayTemplates.ADMIN_QUALIFICATION_CONCISE);
		}
	</tbody>
</table>
