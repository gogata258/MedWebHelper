@model IEnumerable<QualificationPersonnelViewModel>
@{
	ViewData["Title"] = "Personnel";
}

<table class="table table-striped table-bordered">
	<thead>
		<tr>
			<td>Full Name</td>
			<td>Positioned Since</td>
			<td>Actions</td>
		</tr>
	</thead>
	<tbody>
		@foreach (var person in Model)
		{
			@Html.DisplayFor(Model => person, DisplayTemplates.ADMIN_QUALIFICATION_PERSONNEL)
		}
	</tbody>
</table>