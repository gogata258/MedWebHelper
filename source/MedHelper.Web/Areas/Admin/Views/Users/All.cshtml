@model IEnumerable<UserConciseViewModel>
@{
	ViewData["Title"] = "All Users";
}
<h2>@ViewData["Title"]</h2>
<table class="table table-striped table-bordered ">
	<thead>
		<tr>
			<td>Full Name</td>
			<td>Email</td>
			<td>Qualification</td>
			<td>Personnel</td>
			<td>Doctor</td>
			<td>Admin</td>
			<td>Actions</td>
		</tr>
	</thead>
	<tbody>
		@foreach (UserConciseViewModel user in Model)
		{
			@Html.DisplayFor(model => user, DisplayTemplates.ADMIN_USER_CONCISE);
		}
	</tbody>
</table>