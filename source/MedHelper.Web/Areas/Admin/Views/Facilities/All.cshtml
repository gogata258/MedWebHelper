@model IEnumerable<FacilityConciseViewModel>
@{
	ViewData["Title"] = "All Users";
}
<h2>@ViewData["Title"]</h2>
<table class="table table-striped table-bordered ">
	<thead>
		<tr>
			<td>Name</td>
			<td>Personnel</td>
			<td>Actions</td>
		</tr>
	</thead>
	<tbody>
		@foreach (FacilityConciseViewModel facility in Model)
		{
			@Html.DisplayFor(model => facility, DisplayTemplates.ADMIN_FACILITY_CONCISE);
		}
	</tbody>
</table>
