@model IEnumerable<PersonnelConciseViewModel>
@{
	ViewData["Title"] = "All Users";
}
<h2>@ViewData["Title"]</h2>
<table class="table table-striped table-bordered ">
	<thead>
		<tr>
			<td>Full Name</td>
			<td>Qualification</td>
			<td>Actions</td>
		</tr>
	</thead>
	<tbody>
		@foreach (PersonnelConciseViewModel personn in Model)
		{
			@Html.DisplayFor(model => personn, DisplayTemplates.USER_PERSONNEL_CONCISE);
		}
	</tbody>
</table>