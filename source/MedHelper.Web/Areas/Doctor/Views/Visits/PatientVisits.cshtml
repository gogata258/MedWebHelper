@model IEnumerable<PatientVisitConciseViewModel>
@{
	ViewData["Title"] = "Exams";
}
<h2>@ViewData["Title"]</h2>
<table class="table table-striped table-bordered">
	<thead>
		<tr>
			<td>Date</td>
			<td>Doctor</td>
			<td>Status</td>
			<td>Actions</td>
		</tr>
	</thead>
	<tbody>
		@foreach (PatientVisitConciseViewModel visit in Model)
		{
			@Html.DisplayFor(Model => visit, DisplayTemplates.DOCTOR_VISIT_PATIENTCONCISE)
		}
	</tbody>
</table>