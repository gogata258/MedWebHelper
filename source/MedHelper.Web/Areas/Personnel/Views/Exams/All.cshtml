@model IEnumerable<PersonnelExamConciseViewModel>
@{
	ViewData["Title"] = "Exams";
}
<h2>@ViewData["Title"]</h2>
<table class="table table-striped table-bordered">
	<thead>
		<tr>
			<td>Date</td>
			<td>Name</td>
			<td>Status</td>
			<td>Actions</td>
		</tr>
	</thead>
	<tbody>
		@foreach (PersonnelExamConciseViewModel visit in Model)
		{
			@Html.DisplayFor(Model => visit, DisplayTemplates.PERSONNEL_EXAM_CONSIZE)
		}
	</tbody>
</table>